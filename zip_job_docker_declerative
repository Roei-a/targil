pipeline {
    agent {
        docker {image 'my-image:latest'}
    }
    stages {
        stage ('Test-1'){
            steps {
                step ('execute-script'){
                    sh 'python3 /tmp/zip_job.py'
                }
                step ('test-step')
                    sh 'echo "this is the next step"'
            }
            
        }
    }
}
